<div>
  <h1>Log in</h1>
</div>

<%= form_with(url: login_path, scope: :session, local: true) do |f| %>

 <div>
   <%= f.label :email, 'Email' %>
   <%= f.email_field :email %>
 </div>

  <div>
    <%= f.label :password, 'Password' %>
    <%= f.password_field :password %>
  </div>

    <%= f.submit 'Log in' %>
<% end %>

<p><%= link_to 'ユーザー登録はここ!', signup_path %></p>